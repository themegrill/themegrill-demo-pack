	align-items: center;
  display: flex;
  justify-content: center;
}

.coffee-subscription-form form .evf-frontend-grid {
	padding-left: 0 !important;
	padding-right: 0 !important;
}

.coffee-subscription-form form .evf-frontend-grid .evf-field {
	margin-bottom: 0 !important;
}

.coffee-subscription-form form .evf-frontend-grid .evf-field .input-text {
	width: 350px;
	height: 56px;
	margin-bottom: 0;
	padding: 16px 20px;
	border: 0;
	border-radius: 8px 0 0 8px;
}

.coffee-subscription-form form .evf-submit-container  {
	padding-left: 0;
	padding-right: 0;
}

.contact-page-form .evf-field-container, .contact-page-form .evf-submit-container  {
	padding-left: 0 !important;
	padding-right: 0 !important;
}

.coffee-subscription-form form .evf-submit-container .evf-submit  {
	padding: 16px 24px;
	border: 0;
	border-radius: 0 8px 8px 0;
	background: #61493E;
	color: #fff;
}

@media screen and (max-width: 575px) {
	.coffee-subscription-form .evf-field-container {
		width: 100%;
	}
	.coffee-subscription-form form {
		flex-direction: column;
		gap: 16px;
	}
	
	.coffee-subscription-form form .evf-frontend-grid .evf-field .input-text {
		width: 100%;
		border-radius: 8px;
	}

	.coffee-subscription-form form .evf-submit-container {
		width: 100%;
		text-align: center;
	}

	.coffee-subscription-form form .evf-submit-container .evf-submit {
		border-radius: 8px;

	}
}

